<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/origami2"
    android:orientation="horizontal"
    tools:context=".MainMenu">

    <ImageView
        android:id="@+id/Splitball"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="388dp"
        android:contentDescription="@string/splitball_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/splitball" />

    <TextView
        android:id="@+id/user1"
        android:layout_width="373dp"
        android:layout_height="69dp"
        android:background="@color/black"
        android:gravity="center"
        android:textColor="#FF1111"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/user2"
        app:layout_constraintEnd_toStartOf="@+id/user7"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Splitball" />

    <TextView
        android:id="@+id/user3"
        android:layout_width="373dp"
        android:layout_height="59dp"
        android:background="@color/black"
        android:gravity="center"
        android:textColor="#FF1111"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/user4"
        app:layout_constraintEnd_toStartOf="@+id/user8"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user2" />

    <TextView
        android:id="@+id/user7"
        android:layout_width="373dp"
        android:layout_height="68dp"
        android:background="@color/black"
        android:gravity="center"
        android:textColor="#FF1111"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/user8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/user2"
        app:layout_constraintTop_toBottomOf="@+id/user6" />

    <TextView
        android:id="@+id/user2"
        android:layout_width="373dp"
        android:layout_height="59dp"
        android:background="@color/black"
        android:gravity="center"
        android:textColor="#FF1111"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/user3"
        app:layout_constraintEnd_toStartOf="@+id/user7"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user1" />

    <TextView
        android:id="@+id/user6"
        android:layout_width="373dp"
        android:layout_height="68dp"
        android:background="@color/black"
        android:gravity="center"
        android:textColor="#FF1111"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/user7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/user1"
        app:layout_constraintTop_toBottomOf="@+id/Splitball" />

    <TextView
        android:id="@+id/user5"
        android:layout_width="373dp"
        android:layout_height="68dp"
        android:background="@color/black"
        android:gravity="center"
        android:textColor="#FF1111"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/backToMain"
        app:layout_constraintEnd_toStartOf="@+id/user10"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user4" />

    <TextView
        android:id="@+id/user9"
        android:layout_width="373dp"
        android:layout_height="68dp"
        android:background="@color/black"
        android:gravity="center"
        android:textColor="#FF1111"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/user10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/user4"
        app:layout_constraintTop_toBottomOf="@+id/user8" />

    <TextView
        android:id="@+id/user4"
        android:layout_width="373dp"
        android:layout_height="59dp"
        android:background="@color/black"
        android:gravity="center"
        android:textColor="#FF1111"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/user5"
        app:layout_constraintEnd_toStartOf="@+id/user9"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user3" />

    <TextView
        android:id="@+id/user8"
        android:layout_width="373dp"
        android:layout_height="68dp"
        android:background="@color/black"
        android:gravity="center"
        android:textColor="#FF1111"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/user9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/user3"
        app:layout_constraintTop_toBottomOf="@+id/user7" />

    <TextView
        android:id="@+id/user10"
        android:layout_width="376dp"
        android:layout_height="74dp"
        android:background="@color/black"
        android:gravity="center"
        android:textColor="#FF1111"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/backToMain"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/user5"
        app:layout_constraintTop_toBottomOf="@+id/user9" />

    <Button
        android:id="@+id/backToMain"
        android:layout_width="178dp"
        android:layout_height="68dp"
        android:layout_marginStart="423dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="423dp"
        android:layout_marginBottom="2dp"
        android:text="@string/back_to_main"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user10" />

    <Button
        android:id="@+id/local"
        android:layout_width="207dp"
        android:layout_height="69dp"
        android:background="@color/teal_200"
        android:text="@string/local"
        app:backgroundTint="@color/teal_700"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/friends"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.244" />

    <Button
        android:id="@+id/friends"
        android:layout_width="207dp"
        android:layout_height="69dp"
        android:text="@string/friends"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.244" />

    <Button
        android:id="@+id/global"
        android:layout_width="207dp"
        android:layout_height="69dp"
        android:text="@string/global"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/friends"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.244" />


</androidx.constraintlayout.widget.ConstraintLayout>